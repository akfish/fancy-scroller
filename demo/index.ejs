<!DOCTYPE html>
<head>
  <meta name="viewport" content="width=device-width,user-scalable=no">
  <title>Fancy Scroller</title>
</head>
<body>
  <div class="c1 section">1</div>
  <div class="c2 section">2</div>
  <div name='wtf' class="c3 section">3</div>
  <div class="c4 section">4</div>
  <div class="c5 section">5</div>
  <div class="c6 section">6</div>
  <!-- <%- content %> -->
  <script src="fancy-scroller<%= debug ? '' : '.min' %>.js"></script>
  <script type="text/javascript">
    // loadStyle('style.css');
    ready(function() {
      window.scroller = new FancyScroller(document.body, {debug: <%= debug %>});
    });
  </script>
</body>
